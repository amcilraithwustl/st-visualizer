         RowBox[{
         "vals", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], 
         "]"}]}], ",", "ctrs"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"npts", "=", 
       RowBox[{"Map", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"#", "\[LeftDoubleBracket]", 
           RowBox[{"{", 
            RowBox[{"1", ",", "2"}], "}"}], "\[RightDoubleBracket]"}], "&"}], 
         ",", "pts"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"reg", "=", 
       RowBox[{"DelaunayMesh", "[", "npts", "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"tris", "=", 
       RowBox[{"Map", "[", 
        RowBox[{"First", ",", 
         RowBox[{"MeshCells", "[", 
          RowBox[{"reg", ",", "2"}], "]"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
        "verts", ",", "segs", ",", "segmats", ",", "fverts", ",", "ftris", 
         ",", "fmats"}], "}"}], "=", 
       RowBox[{"contourTriMultiDC", "[", 
        RowBox[{"npts", ",", "tris", ",", "vals"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"ctrs", "=", 
       RowBox[{"getContourAllMats2D", "[", 
        RowBox[{
        "verts", ",", "segs", ",", "segmats", ",", "nmat", ",", "shrink"}], 
        "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"{", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"Map", "[", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{"Map", "[", "\[IndentingNewLine]", 
              RowBox[{
               RowBox[{
                RowBox[{"Append", "[", 
                 RowBox[{"#", ",", "z"}], "]"}], "&"}], ",", 
               RowBox[{
               "#", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}]}], 
              "]"}], ",", "\[IndentingNewLine]", 
             RowBox[{
             "#", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}]}], 
            "\[IndentingNewLine]", "}"}], "&"}], ",", "\[IndentingNewLine]", 
          "ctrs"}], "\[IndentingNewLine]", "]"}], ",", "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
        RowBox[{"{", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"Map", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"Append", "[", 
              RowBox[{"#", ",", "z"}], "]"}], "&"}], ",", "fverts"}], "]"}], 
          ",", "\[IndentingNewLine]", "ftris", ",", "\[IndentingNewLine]", 
          "fmats"}], "\[IndentingNewLine]", "}"}]}], "\[IndentingNewLine]", 
       "}"}]}]}], "]"}]}], ";"}]], "Input",
