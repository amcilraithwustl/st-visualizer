Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"genHexGrid", "[", 
    RowBox[{
    "n_", ",", "m_", ",", "len_", ",", "lowleft_", ",", "rot_", ",", "z_"}], 
    "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"h", "=", 
        RowBox[{"len", "*", 
         RowBox[{
          SqrtBox["3"], "/", "2"}]}]}], ",", "pts", ",", 
       RowBox[{"rotM", "=", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{"Cos", "[", "rot", "]"}], ",", 
            RowBox[{"-", 
             RowBox[{"Sin", "[", "rot", "]"}]}]}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"Sin", "[", "rot", "]"}], ",", 
            RowBox[{"Cos", "[", "rot", "]"}]}], "}"}]}], "}"}]}]}], "}"}], 
     ",", "\[IndentingNewLine]", 
     RowBox[{"N", "[", 
      RowBox[{"Map", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Append", "[", 
          RowBox[{"#", ",", "z"}], "]"}], "&"}], ",", 
        RowBox[{"Flatten", "[", 
         RowBox[{
          RowBox[{"Table", "[", 
           RowBox[{
            RowBox[{"If", "[", 
             RowBox[{
              RowBox[{"OddQ", "[", "i", "]"}], ",", "\[IndentingNewLine]", 
              RowBox[{"(*", " ", 
               RowBox[{"long", " ", "line"}], " ", "*)"}], 
              "\[IndentingNewLine]", 
              RowBox[{"Table", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{"rotM", ".", 
                  RowBox[{"{", 
                   RowBox[{
                    RowBox[{
                    RowBox[{"(", 
                    RowBox[{"j", "-", "1"}], ")"}], "*", "len"}], ",", 
                    RowBox[{
                    RowBox[{"(", 
                    RowBox[{"i", "-", "1"}], ")"}], "*", "h"}]}], "}"}]}], 
                 "+", "lowleft"}], ",", 
                RowBox[{"{", 
                 RowBox[{"j", ",", "n"}], "}"}]}], "]"}], ",", 
              "\[IndentingNewLine]", 
              RowBox[{"(*", " ", 
               RowBox[{"short", " ", "line"}], " ", "*)"}], 
              "\[IndentingNewLine]", 
              RowBox[{"Table", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{"rotM", ".", 
                  RowBox[{"{", 
                   RowBox[{
                    RowBox[{
                    RowBox[{"(", 
                    RowBox[{"j", "-", ".5"}], ")"}], "*", "len"}], ",", 
                    RowBox[{
                    RowBox[{"(", 
                    RowBox[{"i", "-", "1"}], ")"}], "*", "h"}]}], "}"}]}], 
                 "+", "lowleft"}], ",", 
                RowBox[{"{", 
                 RowBox[{"j", ",", 
                  RowBox[{"n", "-", "1"}]}], "}"}]}], "]"}]}], "]"}], ",", 
            RowBox[{"{", 
             RowBox[{"i", ",", "m"}], "}"}]}], "]"}], ",", "1"}], "]"}]}], 
       "]"}], "]"}]}], "]"}]}], ";"}]], "Input",
