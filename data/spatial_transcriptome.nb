            RowBox[{"Length", "[", 
             RowBox[{"Union", "[", "#", "]"}], "]"}], "\[Equal]", "1"}], ",", 
           RowBox[{"Print", "[", "#", "]"}]}], "]"}], "&"}], ",", "tets"}], 
       "]"}], ";", "\[IndentingNewLine]", 
