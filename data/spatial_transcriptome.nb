Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"getSectionContours", "[", 
    RowBox[{"pts_", ",", "vals_", ",", "shrink_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"tris", ",", "npts", ",", 
       RowBox[{"z", "=", 
        RowBox[{"pts", "\[LeftDoubleBracket]", 
         RowBox[{"1", ",", "3"}], "\[RightDoubleBracket]"}]}], ",", "reg", 
